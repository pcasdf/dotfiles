[
  {
    "bindings": {
      "ctrl-'": "workspace::NewCenterTerminal",
      "alt-cmd-,": "zed::OpenKeymap"
    }
  },
  {
    "context": "Editor",
    "bindings": {
      "alt-l": "editor::GoToDiagnostic",
      "alt-h": "editor::GoToPrevDiagnostic",
      "alt-j": "editor::GoToHunk",
      "alt-k": "editor::GoToPrevHunk",
      "alt-p": "editor::CopyPermalinkToLine",
      "alt-o": "editor::OpenPermalinkToLine"
    }
  },
  {
    "context": "Editor && vim_mode == normal && vim_operator == none && !VimWaiting",
    "bindings": {}
  },
  {
    "context": "Terminal",
    "bindings": {
      "ctrl-enter": ["terminal::SendText", "\u001b[13;5u"]
    }
  }
]
